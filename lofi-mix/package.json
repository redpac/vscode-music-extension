{
  "name": "lofi-mix",
  "displayName": "lofi-mix",
  "description": "vs code extension to play lofi music while coding",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/chinmaykhamkar/vscode-lofi-mix"
  },
  "bugs": "https://github.com/chinmaykhamkar/vscode-lofi-mix/issues",
  "publisher": "chinmaykhamkar",
  "license": "MIT",
  "icon": "ext-logo.jpeg",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:lofi-mix.startSession",
    "onView:lofiMixPlayer",
    "onView:lofiMixPlayer2"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lofi-mix.startSession",
        "title": "Lofi Mix: Start lofi mix session"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "lofiMixView",
          "title": "Lofi Mix",
          "icon": "./media/logo.png"
        }
      ]
    },
    "views": {
      "lofiMixView": [
        {
          "type": "webview",
          "id": "lofiMixPlayer",
          "name": "Lofi Mix Player"
        }
      ],
      "explorer": [
        {
          "type": "webview",
          "id": "lofiMixPlayer2",
          "name": "Lofi Mix Player"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "lofi-mix": "file:"
  }
}
